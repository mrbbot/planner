{
  "name": "planner-pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dashboard": "webpack-dashboard"
  },
  "dependencies": {
    "bulma": "^0.7.1",
